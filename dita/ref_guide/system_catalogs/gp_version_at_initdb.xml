                        <entry colname="col3">参考</entry>
                        <entry colname="col4">描述</entry>
                    </row>
                </thead>
                <tbody>
                    <row>
                        <entry colname="col1">
                            <codeph>schemaversion</codeph>
                        </entry>
                        <entry colname="col2">integer</entry>
                        <entry colname="col3"/>
                        <entry colname="col4">Schema版本号。</entry>
                    </row>
                    <row>
                        <entry colname="col1">
